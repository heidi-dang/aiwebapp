# Optional: UI auth token for AgentOS requests
# NEXT_PUBLIC_OS_SECURITY_KEY=your_token_here

# Optional: UI base URL for local dev docs / links
# NEXT_PUBLIC_UI_URL=http://localhost:4000
# NEXT_PUBLIC_UI_URL=http://localhost:3006

# Runner proxy destination (used in next.config.ts rewrites)
RUNNER_URL=http://localhost:4002
RUNNER_TOKEN=change_me

# Runtime limits
MAX_ITERATIONS=10
REQUEST_TIMEOUT_MS=30000
MAX_PAYLOAD_SIZE=1048576
FEATURE_X_ENABLED=false
